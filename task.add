import re
with open('task_add.txt','r',encoding="utf-8")as f:
    table=f.read()
date=re.findall(r'\b\d{2,4}[./-]\d{2}[./-]\d{2,4}\b',table)
email=re.findall(r'[A-Za-z0-9._/%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,}',table)
caint=re.findall(r'https?://[^\s]+',table)

print('date',date)
print('email',email)
print("caint",caint)
